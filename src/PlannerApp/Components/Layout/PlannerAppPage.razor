    <MudText Typo="Typo.h4" >@Title</MudText>

    <MudBreadcrumbs Items="BreadcrumbItems">
        <SeparatorTemplate>
            <MudIcon Icon="@Icons.Material.Filled.ArrowForward" Size="Size.Small" />
        </SeparatorTemplate>
    </MudBreadcrumbs>

    @ChildContent

</MudContainer>
